#include <stdio.h>

int main() {
    float radius, height, volume;

    // Input the radius and height from the user
    printf("Введіть радіус основи циліндра: ");
    scanf("%f", &radius);

    printf("Введіть радіус основи циліндра: ");
    scanf("%f", &height);

    // Calculate the volume of the cylinder
    volume = 3.14159 * radius * radius * height;

    // Display the volume of the cylinder
    printf("Введіть радіус основи циліндра: %.2f\n", volume);

    return 0;
}
